[
    {
        "id": 3263,
        "sensor_name": "# Rule 3263 - When ast-button-1 is Triggered AND NOT (start-lamp is on AND lights-on is solved) AND OR (gibbs-sensor is Triggered OR rune-puzzle is solved), do Set task moon-place to solved",
        "constraints": [
            {
                "type": "state-equals",
                "sensor": "ast-button-1",
                "state": "Triggered"
            },
            {
                "type": "not",
                "nestedConstraints": [
                    {
                        "type": "state-equals",
                        "sensor": "start-lamp",
                        "state": "on"
                    },
                    {
                        "type": "task-completed",
                        "task": "lights-on",
                        "states": [
                            "solved"
                        ]
                    }
                ]
            },
            {
                "type": "or",
                "nestedConstraints": [
                    {
                        "type": "state-equals",
                        "sensor": "gibbs-sensor",
                        "state": "Triggered"
                    },
                    {
                        "type": "task-completed",
                        "task": "rune-puzzle",
                        "states": [
                            "solved"
                        ]
                    }
                ]
            }
        ],
        "actions": [
            {
                "task": "moon-place",
                "status": "solved"
            }
        ]
    },
    {
        "id": 2491,
        "sensor_name": "# Rule 2491 - When moon-place is solved AND Max executions: 1, do Set astronomy-door-lock to unlocked Wait 5 seconds Play test.ogg at volume 70 Set astronomy-door-lock to locked Increase sequence_tree by 1",
        "constraints": [
            {
                "type": "task-completed",
                "task": "moon-place",
                "states": [
                    "solved"
                ]
            },
            {
                "type": "max-executions",
                "max_executions": "1",
                "current_executions": 0
            }
        ],
        "actions": [
            {
                "sensor": "astronomy-door-lock",
                "state": "unlocked"
            },
            {
                "delay": "5"
            },
            {
                "play_sound": "test.ogg",
                "volume": "70",
                "pi": [
                    "mlv-central",
                    "mlv-astronomy"
                ]
            },
            {
                "sensor": "astronomy-door-lock",
                "state": "locked"
            },
            {
                "sensor": "sequence_tree",
                "increment": "1"
            }
        ]
    },
    {
        "id": 2825,
        "sensor_name": "# Rule 2825",
        "constraints": [
            {
                "type": "state-equals",
                "sensor": "corridor-door-lock",
                "state": "locked"
            },
            {
                "type": "max-executions",
                "max_executions": "1",
                "current_executions": 1
            }
        ],
        "actions": [
            {
                "task": "lights-on",
                "status": "solved"
            },
            {
                "type": "custom-function",
                "function_name": "test_function()"
            }
        ]
    }
]